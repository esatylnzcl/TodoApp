{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "AllowedHosts": "yourdomain.com;www.yourdomain.com",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=ToDoDb;Username=todoapp_user;Password=${DB_PASSWORD}"
  },
  "JwtSettings": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "ToDoApp",
    "Audience": "Users",
    "ExpirationMinutes": 60
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "https://www.yourdomain.com",
      "https://yourdomain.com"
    ]
  }
}
